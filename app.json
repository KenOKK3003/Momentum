{
  "expo": {
    "scheme": "momentumapp",
    "name": "MomentumApp",
    "slug": "MomentumApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MomentumLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "owner": "momentumcorp",
    "splash": {
      "image": "./assets/images/MomentumLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MomentumLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.JerkWeight.Momentum",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/MomentumLogo.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-google-signin/google-signin",
      "expo-web-browser"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e50efccf-05cc-45cf-9ac5-1498a8ef66b9"
      }
    }
  }
}
